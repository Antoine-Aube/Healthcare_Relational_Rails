
<h1>Patients</h1>
<!--This link does nothin-->
<a href="/hospitals/<%= @hospital.id%>/patients">Sort Patient Alphabetically</a><br>
<br>
<% @hospital.patients.each do |patient| %>
<a href="patients/<%=patient.id%>">Go to Patient</a>
<a href="/patients/<%= patient.id %>/edit">Edit Patient Data</a>
<table>
<% patient.attributes.each_pair do |key, value|%>
<tr>
<td style="color:blue"><%= key %>: </td>
<td><%= value %></td>
</tr>
<% end %>
<br>
</table>
<br>


<% end %>
<br>
<a href="/hospitals/<%=@hospital.id%>/patients/new">Create New Patient</a>