
<h1>Patients Index</h1>


<% @patients.each do |patient| %>
  <a href="patients/<%=patient.id%>">Go to Patient <%= patient.name %></a>
  <a href="/patients/<%= patient.id %>/edit">Edit Patient <%= patient.name %> Data</a>
<%= button_to "Delete #{patient.name}", "/patients/#{patient.id}", method: :delete %>
<table>
  <% patient.attributes.each_pair do |key, value|%>
    <tr>
      <td style="color:blue"><%= key %>: </td>
      <td><%= value %></td>
    </tr>
  <% end %>
      <br>
</table>
<% end %>
<br>
